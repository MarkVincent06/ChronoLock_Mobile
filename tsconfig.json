    "app/(faculty)/access/control.jsx",
    "app/(faculty)/access/control.jsx",
    "app/chat/index.tsx",
    "app/chat/message.jsx",
    "app/chat/create-group.tsx",
    "app/chat/create-group.tsx",
    "app/account/index.jsx"
  ]
}
